{
  appId: "com.bioscanai.drishti",
  productName: "Drishti AI",
  directories: {
    output: "release/${version}",
  },
  files: ["dist", "dist-electron"],
  mac: {
    target: ["dmg"],
    icon: "images/logo.png",
  },
  win: {
    target: ["nsis"],
    icon: "images/logo.png",
  },
  linux: {
    target: ["AppImage"],
    icon: "images/logo.png",
  },
  nsis: {
    oneClick: false,
    allowToChangeInstallationDirectory: true,
  },
}
